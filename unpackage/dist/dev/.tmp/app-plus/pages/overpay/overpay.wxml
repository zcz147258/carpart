<view><cu-custom vue-id="1" bgColor="bg-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText"></view><view class="text-xl" slot="content">车牌补缴</view></cu-custom><block wx:if="{{jaOrders.length>0}}"><view class="bg-white title"><view class="text-right text-red padding-left-sm padding-right-sm margin-left-sm margin-right-sm">{{"欠费合计: "+iSumHisDebt/100+"\n\t\t\t元 订单数："+iCountHisDebt+" 笔"}}</view></view></block><mescroll-body class="vue-ref" vue-id="2" top="50" bottom="50" down="{{downOption}}" data-ref="mescrollRef" data-event-opts="{{[['^init',[['mescrollInit']]],['^down',[['downCallback']]],['^up',[['upCallback']]]]}}" bind:init="__e" bind:down="__e" bind:up="__e" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{jaOrders}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="bg-white margin-sm padding-sm" style="border-radius:10px;"><view class="flex justify-between padding-left-sm padding-right-sm"><view class="flex justify-start"><label><checkbox checked="{{item.checkeds}}" value="{{index}}" disabled="{{busicomid!=null&&busicomid!=item.lBusicomId?'disabled':''}}" data-event-opts="{{[['tap',[['selected',[index]]]]]}}" bindtap="__e"></checkbox><text class="text-gray padding-left-sm">{{item.sPlateNo}}</text></label></view><view data-event-opts="{{[['tap',[['todetails',['$0'],[[['jaOrders','',index,'lOrderId']]]]]]]}}" class="text-blue" bindtap="__e">查看详情<text class="cuIcon-right"></text></view></view><view class="flex justify-between padding-top-xs padding-left-sm padding-right-sm"><view>{{"订单时间："+item.sOrderTime1}}</view><view>{{"已收"+item.iPaidMoney/100+"元"}}</view></view><view class="flex justify-between padding-top-xs padding-left-sm padding-right-sm"><view>{{"关单时间："+item.sOrderTime2}}</view><view>欠费<text class="text-red">{{item.iDebtMoney/100}}</text>元</view></view></view></block></mescroll-body><view class="u-bottom flex bg-white line" style="height:50px;"><view class="flex-treble padding-sm flex text-xl"><label data-event-opts="{{[['tap',[['quanxuan']]]]}}" bindtap="__e"><checkbox class="margin-left-sm" color="#757575" checked="{{checkedAll}}"></checkbox></label><text data-event-opts="{{[['tap',[['quanxuan']]]]}}" class="padding-left-sm" bindtap="__e">全选</text><text class="padding-left-sm">总计：<text class="padding-left-sm text-orange">{{''+selectedCount+" 笔"}}</text><text class="padding-left-sm text-red">{{''+selectedSum/100+" 元"}}</text></text></view><view class="flex-sub"><button class="cu-btn bg-blue margin-sm" type="primary" data-event-opts="{{[['tap',[['gopay',['$event']]]]]}}" bindtap="__e">补缴</button></view></view><modal vue-id="3" modalName="{{modalName}}" modalContent="{{modalContent}}" data-event-opts="{{[['^modalNameChange',[['modalNameChange']]]]}}" bind:modalNameChange="__e" bind:__l="__l"></modal></view>